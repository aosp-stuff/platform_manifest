<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="extra"
           revision="android-10"
           fetch="https://github.com/aosp-stuff" />

  <!-- Remove -->
  <remove-project name="platform/external/tinycompress" />
  <remove-project name="platform/hardware/libhardware" />

  <!-- External -->
  <project path="external/tinycompress" name="platform_external_tinycompress" remote="extra" />

  <!-- Hardware -->
  <project path="hardware/libhardware" name="platform_hardware_libhardware" remote="extra" />

</manifest>
